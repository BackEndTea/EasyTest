{
    "name": "backendtea/easy-test",
    "type": "library",
    "description": "Easy testing for PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Gert de Pagter",
            "email": "BackEndTea@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/finder": "^4.2"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "phpstan/phpstan": "^0.11.12",
        "vimeo/psalm": "^3.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EasyTest\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "bin": [
        "bin/easytest"
    ]
}
